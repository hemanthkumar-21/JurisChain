{
  "canisters": {
    "backend": {
      "type": "azle",
      "main":"./src/backend/src/index.ts"
    },
    "frontend": {
      "dependencies": ["backend"],
      "type": "assets",
      "source": ["src/frontend/dist/"],
      "workspace":"src/frontend"
    }
  },
  "output_env_file": ".env",
  "version": 2,
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral"
    }
  }
}
